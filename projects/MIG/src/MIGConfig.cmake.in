### Moris Toolkit Config File ###

if(@PBC@_CONFIGURED)
	return()
endif()

set(@PBC@_CONFIGURED TRUE)

# CMake configuration
@PACKAGE_INIT@

# Additional macros
include(CMakeFindDependencyMacro)

# Compilers and used in build
set(@PBC@_CXX_COMPILER "@MORIS_CXX_COMPILER@")
set(@PBC@_C_COMPLIER "@MORIS_C_COMPLIER@")

# Flags used in build
set(@PBC@_CXX_FLAGS "@MORIS_CXX_FLAGS@" "@PBC_COMPILE_DEFINITIONS@")
set(@PBC@_C_FLAGS "@MORIS_C_FLAGS@" "@PBC_COMPILE_DEFINITIONS@")

# Find dependencies
@PBC_CONFIG_TPL_DEPENDENCIES@
@PBC_CONFIG_MORIS_DEPENDENCIES@

# Create library target
include("@PACKAGE_PBC_CONFIG_INSTALL_DIR@/@PBC@Targets.cmake")

# Library target list
set(@PBC@_LIBRARIES @PBC@-lib)

set(ALL_MRS_LIBS "${ALL_MRS_LIBS};@PBC@-lib")