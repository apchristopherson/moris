<?xml version="1.0"?>
<moris>
	<xtk_problem>
		<mesh_input>
			<path>generated:10x10x10</path>
			<type>STK</type>
		</mesh_input>
		<geometry>
			<geometry_type>Sphere</geometry_type>
			<real_parameters>9.2,10.0,10.0,10.0</real_parameters>
			<real_labels>r,xc,yc,zc</real_labels>
		</geometry>
		<refine>
			<subdivision_method>Hex8 Regular Subdivision</subdivision_method>
			<subdivision_method>Tet4 Node Hierarchy</subdivision_method>
		</refine>
		<stl>
			<dump_stl>true</dump_stl>
			<stl_file>./stl_xml.stl</stl_file>
		</stl>
		<operators>
			<compute_sens>false</compute_sens>
			<unzip>false</unzip>
			<enrich>false</enrich>
			<ghost_stab>false</ghost_stab>
		</operators>
		<output>
			<output_mesh>./test_xml_output1.exo</output_mesh>
			<output_file>./xtk_run_data2.hdf5</output_file>
		</output>
	</xtk_problem>
</moris>
