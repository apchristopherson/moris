<?xml version="1.0"?>
<moris>
    <hmr>
        <parameters>
	        <number_of_elements_per_dimension>2, 2</number_of_elements_per_dimension>
	        <domain_dimensions>1, 1</domain_dimensions>
	        <domain_offset>0, 0</domain_offset>
	        <buffer_size>2</buffer_size>
	        <interpolation_order>2</interpolation_order>
	        <verbose>true</verbose>
	        <truncate_bsplines>true</truncate_bsplines>
	        <max_volume_refinement_level>3</max_volume_refinement_level>
	        <max_surface_refinement_level>5</max_surface_refinement_level>
        </parameters>
        <field>
            <label>LevelSet</label>
            <input_hdf5></input_hdf5>
            <input_values>LevelSet.bin</input_values>
            <input_coeffs></input_coeffs>
            <output_hdf5></output_hdf5>
            <output_values>LevelSet_Values.bin</output_values>
            <output_coeffs>LevelSet_Coeffs.bin</output_coeffs>
            <refine>true</refine>
        </field>
    </hmr>
</moris>
